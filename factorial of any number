import java.util.*;
public class Main {
        static int factorial(int n){
            int fact=1;
            if(n>0)
            {
                fact=n*factorial(n-1);
                return fact;
            }
            else
                return 1;
        }
    
       public static void main(String[] args) {
        int n,ans;
        System.out.println("Enter any number : ");
        Scanner scn=new Scanner(System.in);
        n=scn.nextInt();
        ans=factorial(n);
        System.out.println("factorial of "+n+" is : "+ans);
    }
}
