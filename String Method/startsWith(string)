import java.util.Scanner;
public class Main{
    public static void main(String[]args){
    Scanner scn=new Scanner(System.in);
        String str;
        System.out.print("Enter any String : ");
        str=scn.nextLine();
        System.out.println(str.startsWith("yog"));
        //it check that the string starts with yog or not
    }
}
