import java.io.*;
class writeInFile{
	public static void main(String[]args){
		
	
	try{
		FileWriter f=new FileWriter("C:\\Users\\lenovo\\Desktop\\VIT SYLLABUS\\yami.txt");
		try{
			f.write("java programming");
		}
		finally{
			f.close();
		}
		System.out.println("Successfully data wrote in file");
	}
	catch(IOException e){
		System.out.println(e);
	}
  }
}
