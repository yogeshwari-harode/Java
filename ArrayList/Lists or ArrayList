import java.util.*;

class ArrayListDemo{
        public static void main(String[] args) {
                ArrayList<String> mylist=new ArrayList<>();
                mylist.add("Apple");
                mylist.add("Mango");
                mylist.add("Banana");
                mylist.add("Mango");
		//List allow Duplicates also

                for(int i=0 ; i<mylist.size() ; i++){
                        System.out.println(mylist.get(i));
                }
        }
}
