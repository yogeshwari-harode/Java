import java.util.Scanner;
public class Main{
    public static void main(String[]args){
    Scanner scn=new Scanner(System.in);
        String str;
        System.out.print("Enter any String : ");
        str=scn.nextLine();
        System.out.println(str.equalsIgnoreCase("yogeshwari"));
        //it check whether the given string is equal to yogeshwari either some character in capital letter or some some in small 
        //it ignore the upper case and lower case
        //check only string it is equal or not
    }
}
