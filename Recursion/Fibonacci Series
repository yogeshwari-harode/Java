import java.util.Scanner;

public class Main {
    public static void Fib(int n){
        int a = 0, b = 1, c;

        System.out.print("Fibonacci series: ");
        for (int i = 1; i <= n; i++) {
            System.out.print(a + " "); 
            c = a + b; 
            a = b;     
            b = c;     
        }
    }
    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);
        System.out.print("Enter the number of terms: ");
        int n = scn.nextInt();
        Fib(n);
    }
}
