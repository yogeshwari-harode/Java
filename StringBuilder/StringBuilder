import java.util.*;
class Main{
    
    public static void main(String[]args){
        Scanner scn=new Scanner(System.in);
        String str=scn.nextLine();
        
        StringBuilder sb=new StringBuilder(str);
        sb.insert(0,"S");
        System.out.println(sb);
        sb.delete(0,1);
        System.out.println(sb);
    }
}
