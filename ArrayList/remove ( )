import java.util.*;

public class rough {
    public static void main(String[] args) {
        Scanner scn = new Scanner(System.in);

        int n=scn.nextInt();
        ArrayList<Integer>arr=new ArrayList<>();
        
        for(int i=0 ; i<n ; i++){
            int number=scn.nextInt();
            arr.add(number);
        }
        System.out.println("ArrayList Before remove index 0 element is:");
        System.out.println(arr);
        //Remove Method
        arr.remove(0);
        System.out.println("ArrayList After remove index 0 element is: ");
        System.out.println(arr);
    }
}
