public class Main{
    public static void main (String[] args)
    {
        int x=30,y=20,z=10;
        if((x<y)&&(x<z))
            System.out.println("x is smaller");
        else if((y<x)&&(y<z))
            System.out.println("y is smaller");
        else 
            System.out.println("z is smaller");
        
    }
    
}
