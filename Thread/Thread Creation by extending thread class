class A extends Thread{
	public void run(){
	    for(int i=0 ; i<5 ; i++){
	        System.out.println("Child method called");
	    }
	}
}
class B{
	public static void main(String[] args) {
		A obj =new A();
		obj.start();
		for(int i=0 ; i<5 ; i++){
		    System.out.println("Main method called");
	        }
	}
}
